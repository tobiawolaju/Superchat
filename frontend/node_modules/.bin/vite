#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules/vite/bin/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules/vite/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules/vite/bin/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules/vite/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/vite@6.4.1_@types+node@22.1_2a13229d2470a3035cb39b46a6df07fb/node_modules:/mnt/d/Projects/Superchat/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
